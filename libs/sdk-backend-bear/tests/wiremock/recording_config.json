{
    "targetBaseUrl": "https://secure.gooddata.com",
    "filters": {
        "urlPathPattern": "/gdc/.*",
        "method": "ANY",
        "allowNonProxied": true
    },
    "captureHeaders": {
        "Accept": {},
        "Content-Type": {
            "caseInsensitive": true
        }
    },
    "requestBodyPattern": {
        "matcher": "equalToJson",
        "ignoreArrayOrder": false,
        "ignoreExtraElements": true
    },
    "extractBodyCriteria": {
        "textSizeThreshold": "2048",
        "binarySizeThreshold": "10240"
    },
    "outputFormat": "FULL",
    "persist": true,
    "repeatsAsScenarios": false
}
