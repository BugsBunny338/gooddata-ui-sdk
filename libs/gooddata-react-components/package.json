        "@types/node": "7.0.8",
        "@types/prop-types": "15.5.2",
        "@types/react": "16.0.34",
        "@types/react-dom": "16.0.3",
        "@types/react-intl": "2.3.5",
        "@types/react-measure": "2.0.5",
        "@types/react-portal": "4.0.1",
        "@types/storybook__addon-actions": "3.4.2",
        "@types/storybook__react": "4.0.1",
        "@types/stringify-object": "^3.2.0",
        "@types/uuid": "3.4.3",
        "babel-loader": "^8.0.5",
        "circular-dependency-plugin": "5.0.2",
        "css-loader": "0.28.7",
        "enzyme": "3.5.0",
        "enzyme-adapter-react-16": "1.3.1",
        "file-loader": "3.0.1",
        "foundation-sites": "^5.5.3",
        "isomorphic-fetch": "2.2.1",
        "jest": "24.7.1",
        "jest-junit": "3.0.0",
        "madge": "^3.4.4",
        "node-sass": "4.7.2",
        "node-sass-magic-importer": "5.1.0",
        "prettier": "^1.18.2",
        "raf": "3.3.2",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "sass-loader": "6.0.6",
        "style-loader": "0.19.1",
        "ts-jest": "24.0.0",
        "ts-loader": "6.0.0",
        "tslint-config-prettier": "1.18.0",
        "tslint-microsoft-contrib": "^5.2.1",
        "typescript": "3.3.4000"
    },
    "dependencies": {
        "@gooddata/gooddata-js": "^11.17.0",
        "@gooddata/goodstrap": "^63.1.1",
        "@gooddata/js-utils": "^3.4.0",
        "@gooddata/numberjs": "^3.1.2",
        "@gooddata/typings": "^2.16.0",
        "ag-grid-community": "20.0.0",
        "ag-grid-react": "20.0.0",
        "classnames": "^2.2.6",
        "custom-event": "^1.0.1",
        "fixed-data-table-2": "^0.7.17",
        "highcharts": "7.1.1",
        "highcharts-grouped-categories": "^1.1.3",
        "hoist-non-react-statics": "^3.3.0",
        "http-status-codes": "^1.3.0",
        "immer": "^1.3.1",
        "invariant": "^2.2.2",
        "json-stable-stringify": "^1.0.1",
        "lodash": "^4.17.11",
        "nodelist-foreach-polyfill": "^1.2.0",
        "prop-types": "^15.6.0",
        "react-dom-factories": "^1.0.2",
        "react-intl": "^2.4.0",
        "react-measure": "^2.3.0",
        "rxjs": "^5.5.6",
        "stringify-object": "^2.4.0",
        "uuid": "^3.2.1"
    },
    "peerDependencies": {
        "react": "^16.5.2",
        "react-dom": "^16.5.2"
    },
    "config": {
        "tslint": "./{__mocks__,src,stories}/{,**/}*.{ts,tsx}"
    },
    "scripts": {
        "prepublishOnly": "npm run-script build",
        "build": "bash scripts/build.sh",
        "build-dev": "bash scripts/build.sh --dev",
        "dev": "bash scripts/build.sh --dev-watch",
        "test": "jest --watch",
        "test-once": "jest",
        "run-tslint": "tslint -p tsconfig.json",
        "tslint": "npm run-script run-tslint -t verbose $npm_package_config_tslint",
        "tslint-ci": "mkdir -p ./ci/results && npm run-script run-tslint -t checkstyle -o ./ci/results/tslint-results.xml $npm_package_config_tslint",
        "prettier-check": "prettier --check '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "check-circular-deps": "madge --circular --extensions ts ./src",
        "validate": "tsc --noEmit && npm run-script tslint && npm run-script prettier-check",
        "validate-ci": "tsc --noEmit && npm run-script tslint-ci && npm run-script prettier-check",
        "storybook": "npm run-script build && start-storybook -p 9001 -c .storybook",
        "build-storybook": "npm run-script build && build-storybook -c .storybook -o dist-storybook",
        "test-storybook": "npm run-script build && test-storybook"
    },
    "typings": "dist/index.d.ts",
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.js"
        ],
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?)$",
        "testPathIgnorePatterns": [
            ",/node_modules/",
            "/dist/"
        ],
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!**/*.d.ts"
        ],
        "collectCoverage": false,
        "moduleNameMapper": {
            "^[./a-zA-Z0-9$_-]+\\.svg$": "<rootDir>/jestSvgStub.js",
            "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts"
        },
        "coverageReporters": [
            "html"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "json"
        ]
    }
}
