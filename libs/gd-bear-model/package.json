{
    "name": "@gooddata/gd-bear-model",
    "version": "8.0.0",
    "description": "TypeScript definition files for GoodData platform",
    "typings": "dist/index.d.ts",
    "main": "dist/index.js",
    "scripts": {
        "build": "rm -rf dist && tsc -p tsconfig.build.json",
        "prepublish": "npm run build",
        "dev": "tsc -p tsconfig.dev.json --watch --noEmit",
        "test": "jest --watch",
        "test-once": "jest",
        "test-ci": "JEST_JUNIT_OUTPUT=./ci/results/test-results.xml jest --config jest.ci.json",
        "tslint": "tslint -p . -t verbose $npm_package_config_tslint",
        "tslint-ci": "mkdir -p ./ci/results && tslint -p . -t checkstyle -o ./ci/results/tslint-results.xml $npm_package_config_tslint",
        "validate": "tsc --noEmit && npm run tslint",
        "validate-ci": "tsc --noEmit && npm run tslint-ci",
        "prettier-check": "prettier --check 'src/**/*.ts'",
        "prettier-write": "prettier --write 'src/**/*.ts'"
    },
    "config": {
        "tslint": "./src/{,**/}*.ts"
    },
    "files": [
        "dist",
        "index.ts",
        "README.md"
    ],
    "repository": "https://github.com/gooddata/gooddata-ui-sdk/tree/master/libs/gd-bear-model",
    "jest": {
        "preset": "ts-jest",
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.js"
        ],
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
        "collectCoverageFrom": [
            "src/**/*.{ts}",
            "!**/*.d.ts"
        ],
        "collectCoverage": false,
        "coverageReporters": [
            "html"
        ],
        "globals": {
            "ts-jest": {
                "packageJson": "package.json"
            }
        }
    },
    "keywords": [
        "typescript",
        "gooddata",
        "types"
    ],
    "author": "GoodData",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/gooddata/gooddata-typings/issues"
    },
    "homepage": "https://github.com/gooddata/gooddata-typings#readme",
    "dependencies": {
        "lodash": "^4.17.15"
    },
    "devDependencies": {
        "@gooddata/tslint-config": "1.0.0",
        "@types/jest": "^24.0.6",
        "@types/lodash": "^4.14.123",
        "jest": "^24.7.1",
        "jest-junit": "^3.0.0",
        "prettier": "~1.18.2",
        "ts-jest": "^24.1.0",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "3.6.3"
    }
}
