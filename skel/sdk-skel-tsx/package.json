{
    "name": "@gooddata/sdk-skel-tsx",
    "version": "1.0.0",
    "description": "GoodData SDK TypeScript & React skeleton",
    "main": "dist/index.js",
    "repository": "git@github.com:lupko/gooddata-ui-sdk.git",
    "author": "GoodData Corporation",
    "license": "BSD-3-Clause",
    "files": [
        "dist",
        "src",
        "LICENSE"
    ],
    "config": {
        "tslint": "./{__mocks__,src,stories}/{,**/}*.{ts,tsx}"
    },
    "scripts": {
        "build": "bash scripts/build.sh && npm run-script api-extractor",
        "api-extractor": "mkdir -p api && [[ -z \"${CI}\" ]] && (api-extractor run -l) || (api-extractor run)",
        "build-dev": "bash scripts/build.sh --dev",
        "dev": "bash scripts/build.sh --dev-watch",
        "test": "jest --watch",
        "test-once": "jest",
        "run-tslint": "tslint -p tsconfig.json",
        "tslint": "npm run-script run-tslint -t verbose $npm_package_config_tslint",
        "tslint-ci": "mkdir -p ./ci/results && npm run-script run-tslint -t checkstyle -o ./ci/results/tslint-results.xml $npm_package_config_tslint",
        "prettier-check": "prettier --check '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "validate": "tsc --noEmit && npm run-script tslint && npm run-script prettier-check",
        "validate-ci": "tsc --noEmit && npm run-script tslint-ci && npm run-script prettier-check",
        "storybook": "npm run-script build && start-storybook -p 9001 -c .storybook",
        "build-storybook": "npm run-script build && build-storybook -c .storybook -o dist-storybook",
        "test-storybook": "npm run-script build && test-storybook"
    },
    "typings": "dist/index.d.ts",
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.ts"
        ],
        "preset": "ts-jest",
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?)$",
        "testPathIgnorePatterns": [
            ",/node_modules/",
            "/dist/"
        ],
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!**/*.d.ts"
        ],
        "collectCoverage": false,
        "moduleNameMapper": {
            "^[./a-zA-Z0-9$_-]+\\.svg$": "<rootDir>/__mocks__/jestSvgStub.js",
            "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts"
        },
        "coverageReporters": [
            "html"
        ],
        "globals": {
            "ts-jest": {
                "packageJson": "package.json"
            }
        }
    },
    "dependencies": {
        "lodash": "^4.17.11",
        "prop-types": "^15.6.0",
        "react-intl": "^2.4.0"
    },
    "peerDependencies": {
        "react": "^16.5.2",
        "react-dom": "^16.5.2"
    },
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@gooddata/test-storybook": "3.1.1",
        "@gooddata/tslint-config": "1.0.0",
        "@microsoft/api-extractor": "^7.3.8",
        "@storybook/addon-actions": "5.0.11",
        "@storybook/addon-options": "5.0.11",
        "@storybook/addons": "5.0.11",
        "@storybook/react": "5.0.11",
        "@storybook/theming": "5.0.11",
        "@types/enzyme": "3.1.13",
        "@types/enzyme-adapter-react-16": "1.0.3",
        "@types/jest": "24.0.6",
        "@types/lodash": "^4.14.123",
        "@types/node": "7.0.8",
        "@types/prop-types": "15.5.2",
        "@types/react": "16.0.34",
        "@types/react-dom": "16.0.3",
        "@types/react-intl": "2.3.5",
        "@types/storybook__addon-actions": "3.4.2",
        "@types/storybook__react": "4.0.1",
        "@types/stringify-object": "^3.2.0",
        "babel-loader": "^8.0.5",
        "css-loader": "0.28.7",
        "enzyme": "3.5.0",
        "enzyme-adapter-react-16": "1.3.1",
        "file-loader": "3.0.1",
        "jest": "^24.7.1",
        "jest-junit": "^3.0.0",
        "node-sass": "4.7.2",
        "node-sass-magic-importer": "5.1.0",
        "prettier": "^1.18.2",
        "raf": "^3.3.2",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.19.1",
        "ts-jest": "^24.1.0",
        "ts-loader": "^6.0.0",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "1.18.0",
        "tslint-microsoft-contrib": "^5.2.1",
        "typescript": "3.3.4000"
    }
}
